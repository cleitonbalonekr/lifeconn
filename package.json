{
  "name": "lifeconn",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "format": "prettier --write ./**/*.{ts,tsx}",
    "lint": "eslint  ./**/*.{ts,tsx} --fix --no-error-on-unmatched-pattern --ignore-path .gitignore",
    "flint": "npm run format && npm run lint",
    "prepare": "husky install",
    "pre-commit": "lint-staged && npm test",
    "start-emulator": "docker-compose up -d && npm run test-emulator",
    "test-emulator": "ts-node tests/utils/verify-emulators.ts",
    "test:push": "npm run start-emulator && jest --passWithNoTests --no-cache --runInBand --detectOpenHandles",
    "test": "jest --passWithNoTests --no-cache --runInBand --detectOpenHandles",
    "build:tailwind": "tailwindcss --input src/presentation/shared/styles/input.css --output tailwind.css --no-autoprefixer && tailwind-rn -i src/presentation/shared/styles/tailwind.css -o src/presentation/shared/styles/tailwind.json",
    "dev:tailwind": "concurrently \"tailwindcss --input src/presentation/shared/styles/input.css --output src/presentation/shared/styles/tailwind.css --no-autoprefixer --watch\" \"tailwind-rn -i src/presentation/shared/styles/tailwind.css -o src/presentation/shared/styles/tailwind.json  --watch\""
  },
  "dependencies": {
    "@expo-google-fonts/inter": "^0.2.2",
    "@expo-google-fonts/ubuntu": "^0.2.2",
    "@expo/vector-icons": "^12.0.5",
    "@react-native-async-storage/async-storage": "~1.15.0",
    "@react-native-community/netinfo": "7.1.3",
    "@react-native-community/slider": "^4.2.2",
    "@react-navigation/native": "^6.0.8",
    "@react-navigation/native-stack": "^6.5.2",
    "axios": "^0.26.1",
    "date-fns": "^2.28.0",
    "dotenv": "^16.0.0",
    "expo": "~44.0.0",
    "expo-app-loading": "~1.3.0",
    "expo-background-fetch": "~10.1.0",
    "expo-barcode-scanner": "~11.2.0",
    "expo-constants": "^13.0.2",
    "expo-device": "^4.1.1",
    "expo-font": "~10.0.4",
    "expo-image-picker": "~12.0.1",
    "expo-linking": "^3.0.0",
    "expo-location": "~14.0.1",
    "expo-notifications": "~0.14.0",
    "expo-sensors": "~11.1.0",
    "expo-speech": "~10.1.0",
    "expo-status-bar": "~1.2.0",
    "expo-task-manager": "~10.1.0",
    "expo-updates": "~0.11.6",
    "firebase": "^9.6.10",
    "geolib": "^3.3.3",
    "jest-expo": "^44.0.1",
    "moti": "^0.17.1",
    "nanoid": "^3.3.3",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-native": "0.64.3",
    "react-native-get-random-values": "^1.7.2",
    "react-native-phone-call": "^1.0.9",
    "react-native-qrcode-svg": "^6.1.2",
    "react-native-reanimated": "~2.3.1",
    "react-native-safe-area-context": "3.3.2",
    "react-native-screens": "~3.10.1",
    "react-native-slider": "^0.11.0",
    "react-native-status-bar-height": "^2.6.0",
    "react-native-svg": "^12.3.0",
    "react-native-toast-message": "^2.1.5",
    "react-native-web": "0.17.1",
    "tailwind-rn": "^4.2.0",
    "totalvoice-node": "^1.12.0",
    "uuid": "^8.3.2",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@faker-js/faker": "^6.1.2",
    "@firebase/rules-unit-testing": "^2.0.2",
    "@swc/core": "^1.2.161",
    "@swc/jest": "^0.2.20",
    "@types/jest": "^27.4.1",
    "@types/react": "~17.0.21",
    "@types/react-native": "~0.64.12",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "concurrently": "^7.0.0",
    "eslint": "^7.14.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-import-resolver-babel-plugin-root-import": "^1.1.1",
    "eslint-import-resolver-typescript": "2.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-import-helpers": "1.1.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "firebase-tools": "^10.6.0",
    "husky": "^7.0.4",
    "jest": "^27.0.0",
    "lint-staged": "^11.0.0",
    "postcss": "^8.4.12",
    "prettier": "^2.3.1",
    "react-test-renderer": "17",
    "rimraf": "^3.0.2",
    "tailwindcss": "^3.0.23",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "typescript": "~4.3.5"
  },
  "private": true
}
